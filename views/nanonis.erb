<% @nanonis ||= nanonis %>
<%= partial :"nanonis/header", :locals => {nanonis: @nanonis} %>
<%= partial :"nanonis/#{@nanonis.format}", :locals => {nanonis: @nanonis} %>

<script type="text/javascript">
		var isAlt = true;

		$(document).keyup(function (e) {
			if(e.which == 18) isAlt=false;
		}).keydown(function (e) {
			if(e.which == 18) isAlt=true;
			if(e.which == 39) {
				window.location.assign('<%= @next_url || '#' %>');
			}
			if(e.which == 37) {
				window.location.assign('<%= @previous_url || '#' %>');
			}
		});
</script>